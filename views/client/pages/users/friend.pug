extends ../../layouts/layoutDefault.pug
include ../../mixins/box-head.pug

block main
  .container.my-3

    include ../../partical/friend.pug

    .row
      .col-12
        +box-head("Danh sách bạn bè")


    .row(data-users-friend)
      each user in users
        .col-6(user-id=user.id)
          div(class="box-user")
            div(class="inner-avatar")
              img(
                src="data:image/jpeg;base64,"
                alt=user.fullName
              )
            div(class="inner-info")
              div(class="inner-name") #{user.fullName}
              div(class="inner-buttons")
                a(
                  href=`/chat/${user.roomChatId}`
                  class="btn btn-sm btn-primary mr-1"
                ) Nhắn tin
            div(
              class="inner-status"
              status=user.statusOnline
            ) <i class="fa-solid fa-circle"></i>
              
block script
  script(src="/js/users.js")